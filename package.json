{
  "name": "angular2-starter",
  "version": "0.2.0",
  "description": "A boiler-plate setup for Angular 2 applications",
  "license": "MIT",
  "keywords": [
    "angular2",
    "typescript"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "prebuild:jit": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack --config webpack-aot.config.js",
    "build:jit": "cross-env NODE_ENV=production webpack",
    "profile-build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack-aot.config.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "profile-build:jit": "npm run clean && cross-env NODE_ENV=production webpack --profile --json > stats.json && webpack-bundle-analyzer stats.json",
    "clean": "rimraf dist/",
    "cover": "istanbul report --include=**/coverage/coverage.json text text-summary",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --debug --inline --hot --progress --no-info",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-ts": "tslint \"./src/**/*.ts\" --format stylish --type-check",
    "lint-js": "eslint \"./src/**/*.js\" --ignore-path .gitignore",
    "lint-css": "stylelint \"./src/**/*.css\"",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server",
    "pretest": "rimraf coverage/ & npm run lint",
    "test": "karma start --single-run",
    "posttest": "npm run cover",
    "test:watch": "karma start --auto-run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/angular2-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/angular2-redux-starter/issues"
  },
  "dependencies": {
    "cross-env": "^3.1.3",
    "superstatic": "^6.0.1"
  },
  "devDependencies": {
    "@angular/common": "2.4.3",
    "@angular/compiler": "2.4.3",
    "@angular/compiler-cli": "2.4.3",
    "@angular/core": "2.4.3",
    "@angular/forms": "2.4.3",
    "@angular/http": "2.4.3",
    "@angular/platform-browser": "2.4.3",
    "@angular/platform-browser-dynamic": "2.4.3",
    "@angular/router": "3.4.3",
    "@angular/tsc-wrapped": "0.5.1",
    "@ngtools/webpack": "^1.2.8",
    "@types/core-js": "^0.9.34",
    "@types/jasmine": "2.5.35",
    "@types/node": "^6.0.45",
    "@types/ramda": "^0.0.2",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.5.1",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "codelyzer": "^2.0.0-beta.4",
    "copy-webpack-plugin": "^4.0.0",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.1",
    "cssnano": "^3.7.7",
    "eslint": "^3.8.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "inline-source-map": "^0.6.2",
    "istanbul": "^1.0.0-alpha.2",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "^0.0.26",
    "karma-webpack": "^1.8.0",
    "null-loader": "^0.1.1",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "^1.0.0",
    "ramda": "^0.22.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.7.0",
    "stylelint-webpack-plugin": "^0.5.1",
    "tachyons": "^4.5.5",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "tslint": "^4.1.1",
    "tslint-loader": "^3.3.0",
    "typescript": "2.0.10",
    "webpack": "^2.2.0",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-hot-middleware": "^2.13.0",
    "zone.js": "^0.7.4"
  }
}
