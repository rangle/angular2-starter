{
  "name": "angular2-starter",
  "version": "0.2.0",
  "description": "A boiler-plate setup for Angular 2 applications",
  "license": "MIT",
  "keywords": [
    "angular2",
    "typescript"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rimraf dist/",
    "cover": "istanbul report text text-summary",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-ts": "tslint './src/**/*.ts'",
    "lint-js": "eslint '**/*.js' --ignore-path .gitignore",
    "lint-css": "stylelint './src/**/*.css'",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production http-server ./dist",
    "pretest": "rimraf coverage/ & npm run lint",
    "test": "karma start --singleRun true",
    "posttest": "npm run cover",
    "test:watch": "karma start --singleRun false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rangle/angular2-redux-starter.git"
  },
  "bugs": {
    "url": "https://github.com/rangle/angular2-redux-starter/issues"
  },
  "dependencies": {
    "http-server": "^0.9.0"
  },
  "devDependencies": {
    "@angular/common": "^2.1.0",
    "@angular/compiler": "^2.1.0",
    "@angular/core": "^2.1.0",
    "@angular/forms": "^2.1.0",
    "@angular/http": "^2.1.0",
    "@angular/platform-browser": "^2.1.0",
    "@angular/platform-browser-dynamic": "^2.1.0",
    "@angular/router": "^3.1.0",
    "@types/jasmine": "^2.5.35",
    "@types/node": "^6.0.45",
    "angular2-template-loader": "^0.5.0",
    "autoprefixer": "^6.5.1",
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "basscss": "^8.0.2",
    "basscss-addons": "^1.0.0",
    "basscss-basic": "^1.0.0",
    "classlist-polyfill": "^1.0.3",
    "codelyzer": "^1.0.0-beta.2",
    "console-polyfill": "^0.2.3",
    "copy-webpack-plugin": "^4.0.0",
    "core-js": "^2.4.1",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "cssnano": "^3.7.7",
    "es7-reflect-metadata": "^1.6.0",
    "eslint": "^3.8.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "html5-history-api": "^4.2.7",
    "immutable": "^3.8.1",
    "inline-source-map": "^0.6.2",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-coverage": "^0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-sourcemap-writer": "^0.1.2",
    "karma-spec-reporter": "^0.0.26",
    "karma-webpack": "^1.8.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "8.0.2",
    "postcss-loader": "^1.0.0",
    "raf": "^3.3.0",
    "ramda": "^0.22.1",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.8",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-beta.12",
    "selenium-standalone": "^5.7.2",
    "style-loader": "^0.13.1",
    "stylelint": "^7.5.0",
    "stylelint-webpack-plugin": "^0.4.0",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.3",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.9",
    "webpack-hot-middleware": "^2.13.0",
    "zone.js": "^0.6.26"
  }
}
